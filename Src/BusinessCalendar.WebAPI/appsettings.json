{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Storage": {
    "DatabaseType": "Postgres",
    "HealthTimeoutSeconds": 3,
    "AllowAutoMigrations": true
  },
  "Auth": {
    "UseOpenIdConnectAuth": false,
    "SessionCookieLifetimeMinutes": 60
  }
}